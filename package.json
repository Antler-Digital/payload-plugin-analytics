{
  "name": "payload-plugin-analytics",
  "version": "1.0.3",
  "description": "A plugin for Payload 3.0 to track user activity",
  "license": "MIT",
  "type": "module",
  "packageManager": "yarn@4.5.3",
  "exports": {
    ".": "./src/index.ts"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "scripts": {
    "dev": "tailwindcss -i ./src/styles/input.css -o ./src/components/layout/output.css --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts --outDir dist",
    "build:analytics": "node --loader ts-node/esm src/scripts/minify-script.ts",
    "test": "echo \"Error: no test specified\"",
    "lint": "echo \"Error: no lint specified\"",
    "prepublishOnly": "yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "latest",
    "@payloadcms/next": "latest",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@tanstack/react-table": "^8.20.5",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "lucide-react": "^0.462.0",
    "next": "15.0.4",
    "payload": "latest",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-simple-maps": "^3.0.0",
    "recharts": "^2.13.3",
    "swr": "^2.2.5",
    "tailwind-merge": "latest"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "@types/react-simple-maps": "^3",
    "postcss": "latest",
    "semantic-release": "^24.2.1",
    "tailwindcss": "^3.4.15",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "typescript": "5.7.2"
  }
}
